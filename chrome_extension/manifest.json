{
  "name": "CloudER",
  "version": "1.0.1",
  "description": "DJ helper",
  "manifest_version": 3,
  "author": "Roman Volozhanin",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.beatport.com/genre/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}